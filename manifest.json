{
    "name": "Storage Demo Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html"
    },
    "start_url": "/",
    "display": "standalone",
    "background_color": "#f0f0f0",
    "theme_color": "#007bff",
    "icons": [
        {
        "src": "/icon.png",
        "sizes": "192x192",
        "type": "image/png"
        },
        {
        "src": "/icon.png",
        "sizes": "512x512",
        "type": "image/png"
        }
    ]
}